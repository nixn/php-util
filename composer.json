{
    "name": "nixn/php-util",
    "type": "library",
    "description": "PHP language utilities, mostly to code more in functional style.",
    "keywords": ["functional", "language", "utilities", "array", "string", "pipe", "with", "partial"],
    "license": "MIT",
    "authors": [
        {
            "name": "nix",
            "homepage": "https://keybase.io/nixn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.42"
    },
    "autoload": {
        "psr-4": {
            "nixn\\php\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.2.x-dev"
        }
    }
}
