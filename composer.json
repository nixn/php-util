{
    "name": "nixn/php-util",
    "description": "PHP language utilities, mostly to code more in functional style.",
    "type": "library",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.41"
    },
    "authors": [
        {
            "name": "nix"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "nixn\\php\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.2.x-dev"
        }
    }
}
